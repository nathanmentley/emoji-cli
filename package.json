{
    "name": "clihelper",
    "version": "0.0.1",
    "license": "MIT",
    "scripts": {
        "preinstall": "emojicodec -p clihelper ./🔧/🏁.emojic -o ./libclihelper.a -i ./interface.emojii",
        "build": "yarn run preinstall",
        "clean": "rm -f ./bin/test && rm -f ./*.a && rm -f ./*.emojii && rm -f ./🔧/*.o",
        "test": "rm -f ./bin/test && emojicodec ./🧪/🏁.emojic -o ./bin/test && ./bin/test"
    }
}
